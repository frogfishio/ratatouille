{
  "name": "@frogfish/ratatouille",
  "version": "1.0.2",
  "description": "Best-effort telemetry firehose with NDJSON Relay sinks (Node + Workers) and optional local printing.",
  "license": "GPL-3.0-only",
  "publishConfig": {
    "access": "public"
  },
  "type": "module",
  "sideEffects": false,
  "files": [
    "dist",
    "README.md",
    "LICENSE"
  ],
  "main": "./dist/index.node.cjs",
  "module": "./dist/index.node.js",
  "types": "./dist/index.node.d.ts",
  "exports": {
    ".": {
      "browser": {
        "types": "./dist/index.browser.d.ts",
        "import": "./dist/index.browser.js"
      },
      "types": "./dist/index.node.d.ts",
      "import": "./dist/index.node.js",
      "require": "./dist/index.node.cjs"
    },
    "./relay": {
      "browser": {
        "types": "./dist/relay.worker.d.ts",
        "import": "./dist/relay.worker.js"
      },
      "types": "./dist/relay.d.ts",
      "import": "./dist/relay.js",
      "require": "./dist/relay.cjs"
    },
    "./transports/ringtail": {
      "browser": {
        "types": "./dist/transports/ringtail.worker.d.ts",
        "import": "./dist/transports/ringtail.worker.js"
      },
      "types": "./dist/transports/ringtail.d.ts",
      "import": "./dist/transports/ringtail.js",
      "require": "./dist/transports/ringtail.cjs"
    },
    "./presets/nomad": {
      "types": "./dist/presets/nomad.d.ts",
      "import": "./dist/presets/nomad.js",
      "require": "./dist/presets/nomad.cjs"
    },
    "./presets/lambda": {
      "types": "./dist/presets/lambda.d.ts",
      "import": "./dist/presets/lambda.js",
      "require": "./dist/presets/lambda.cjs"
    },
    "./presets/workers": {
      "browser": {
        "types": "./dist/presets/workers.d.ts",
        "import": "./dist/presets/workers.js"
      },
      "types": "./dist/presets/workers.d.ts",
      "import": "./dist/presets/workers.js"
    }
  },
  "scripts": {
    "clean": "rm -rf dist",
    "build": "npm run clean && tsup src/index.node.ts src/index.browser.ts src/relay.ts src/relay.worker.ts src/transports/ringtail.ts src/transports/ringtail.worker.ts src/presets/nomad.ts src/presets/lambda.ts src/presets/workers.ts",
    "dev": "tsup src/index.node.ts src/index.browser.ts src/relay.ts src/relay.worker.ts src/transports/ringtail.ts src/transports/ringtail.worker.ts src/presets/nomad.ts src/presets/lambda.ts src/presets/workers.ts --watch",
    "prepublishOnly": "npm run build"
  },
  "keywords": [
    "telemetry",
    "firehose",
    "ndjson",
    "relay",
    "workers",
    "logging",
    "debug"
  ],
  "engines": {
    "node": ">=18"
  },
  "devDependencies": {
    "@types/node": "^24.10.4",
    "tsup": "^8.5.1",
    "typescript": "^5.9.3"
  }
}
